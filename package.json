{
  "name": "foot-data-api",
  "version": "1.0.0",
  "description": "API used for FootData application",
  "main": "app.js",
  "engines": {
    "node": "8.10.0",
    "npm": "6.11.3"
  },
  "scripts": {
    "test": "export NODE_ENV=test && nyc mocha ./test/test.js --timeout 20000 --exit",
    "start": "node app.js",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "start-dev": "nodemon app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MulanSales/foot-data-api.git"
  },
  "keywords": [
    "Node.js",
    "JavaScript",
    "Express.js"
  ],
  "author": "Luan M. Sales",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MulanSales/foot-data-api/issues"
  },
  "homepage": "https://github.com/MulanSales/foot-data-api#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-swaggerize-ui": "^1.1.0",
    "graphql": "^14.5.8",
    "mongoose": "^5.7.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.7",
    "express-swagger-generator": "^1.1.15",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  }
}
